{ 
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "routes": [
        {
        "src": "/",
        "status": 308,
        "headers": { "Location": "https://wetube-client.vercel.app/" }
        },
        {
        "src": "/custom-page",
        "headers": { "cache-control": "s-maxage=1000" },
        "dest": "/index.html"
        },
        { "src": "/(.*)", "dest": "/" }
    ]
}